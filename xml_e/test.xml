        <ns0:HotelDescriptiveContent HotelCityCode="UK.LON" HotelCode="YYP" HotelCodeContext="travco" HotelName="Pembridge Palace (Testing)">
            <ns0:HotelInfo>
                <ns0:Descriptions>
                    <ns0:MultimediaDescriptions>
                        <ns0:MultimediaDescription>
                            <ns0:ImageItems>
